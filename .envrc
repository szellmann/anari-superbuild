# use with direnv: https://github.com/zimbatm/direnv

# NOTE: these settings need to be adapted to your
# environment in case you want to use direnv:

CWD=`pwd -P`
export LD_LIBRARY_PATH=$CWD/build/install/lib:$LD_LIBRARY_PATH
export DYLD_LIBRARY_PATH=$CWD/build/install/lib:$DYLD_LIBRARY_PATH
export PYTHONPATH=$CWD/build/install/lib:$PYTHONPATH
